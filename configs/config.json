{
    "agent": {
        "workspace_path": "~/.merobot/workspace",
        "default": {
            "provider": "nvidia",
            "model": "nvidia_nim/z-ai/glm5",
            "temperature": 0.7,
            "max_tokens": 2048
        }
    },
    "providers": {
        "openai": {
            "name": "OpenAI",
            "slug": "openai",
            "api_key": "OPENAI_API_KEY",
            "enabled": false,
            "api_base": "https://api.openai.com/v1",
            "adapters": "openai"
        },
        "groq": {
            "name": "Groq",
            "slug": "groq",
            "api_key": "GROQ_API_KEY",
            "enabled": false,
            "api_base": "https://api.groq.com/v1",
            "adapters": "openai"
        },
        "nvidia": {
            "name": "NVIDIA",
            "slug": "nvidia_nim",
            "api_key": "NVIDIA_API_KEY",
            "enabled": false,
            "api_base": "https://integrate.api.nvidia.com/v1/",
            "adapters": "openai"
        }
    },
    "channels": {
        "telegram": {
            "enabled": true,
            "type": "telegram",
            "env_user_id": "TELEGRAM_USER_ID",
            "env_token": "TELEGRAM_BOT_TOKEN"
        }
    }
}